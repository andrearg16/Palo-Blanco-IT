<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="destination_costs_report" model="ir.actions.report">
            <field name="name">Costes en Destino</field>
            <field name="model">stock.landed.cost</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">dvsisa_report.destination_costs_template</field>
            <field name="report_file">dvsisa_report.destination_costs_template</field>
            <field name="print_report_name">'Costes en Destino - %s' % (object.name)</field>
            <field name="binding_model_id" ref="stock_landed_costs.model_stock_landed_cost"/>
            <field name="binding_type">report</field>
        </record>

        <record id="sale_remmission_report" model="ir.actions.report">
            <field name="name">Remisión de Ventas</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">dvsisa_report.sale_remmission_template</field>
            <field name="report_file">dvsisa_report.sale_remmission_template</field>
            <field name="print_report_name">'Remisión de Ventas - %s' % (object.name)</field>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_type">report</field>
        </record>

        <record id="packing_local_label_template_report" model="ir.actions.report">
            <field name="name">Etiqueta Local</field>
            <field name="model">packing.list</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">dvsisa_report.local_label_template</field>
            <field name="report_file">dvsisa_report.local_label_template</field>
            <field name="print_report_name">'Etiqueta Local - %s' % (object.name)</field>
        </record>

        <record id="reception_label_template_report" model="ir.actions.report">
            <field name="name">Etiqueta de Recepción</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">dvsisa_report.reception_label_template</field>
            <field name="report_file">dvsisa_report.reception_label_template</field>
            <field name="print_report_name">'Etiqueta de Recepción - %s' % (object.name)</field>
        </record>

        <record id="location_label_template_report" model="ir.actions.report">
            <field name="name">Etiqueta de Ubicación</field>
            <field name="model">stock.location</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">dvsisa_report.location_label_template</field>
            <field name="report_file">dvsisa_report.location_label_template</field>
            <field name="print_report_name">'Etiqueta de Ubicación - %s' % (object.name)</field>
        </record>

        <record id="sale_remmission_paperformat" model="report.paperformat">
            <field name="name">Formato: Remisión de Ventas</field>
            <field name="default" eval="True"/>
            <field name="format">Letter</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">62</field>
            <field name="margin_bottom">15</field>
            <field name="margin_left">4</field>
            <field name="margin_right">4</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">55</field>
            <field name="dpi">90</field>
            <field name="report_ids" eval="[(4, ref('sale_remmission_report'))]"/>
        </record>

        <record id="packing_local_label_paperformat" model="report.paperformat">
            <field name="name">Formato: Etiqueta Local</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">75</field>
	        <field name="page_width">105</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">38</field>
            <field name="dpi">90</field>
            <field name="report_ids" eval="[(4, ref('packing_local_label_template_report'))]"/>
        </record>

        <record id="reception_label_label_paperformat" model="report.paperformat">
            <field name="name">Formato: Etiqueta de Recepción</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">75</field>
	        <field name="page_width">105</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">4</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">38</field>
            <field name="dpi">90</field>
            <field name="report_ids" eval="[(4, ref('reception_label_template_report'))]"/>
        </record>

        <record id="reception_location_label_paperformat" model="report.paperformat">
            <field name="name">Formato: Etiqueta de Ubicación</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">75</field>
	        <field name="page_width">105</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">4</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">38</field>
            <field name="dpi">90</field>
            <field name="report_ids" eval="[(4, ref('location_label_template_report'))]"/>
        </record>

    </data>
</odoo>